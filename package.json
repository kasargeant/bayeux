{
  "name": "bayeux",
  "version": "0.0.1",
  "description": "A simple, capable and pragmatic Tap-based test framework.",
  "keywords": [
    "javascript, test, testing, tap"
  ],
  "author": "Kyle Alexis Sargeant",
  "email": "kasargeant@gmail.com",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/kasargeant/bayeux.git"
  },
  "main": "src/index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "engines": {
    "node": "~6.10.0",
    "npm": "~3.10.10"
  },
  "scripts": {
    "build": "warhorse build",
    "clean": "warhorse clean",
    "create": "warhorse create",
    "distribute": "warhorse distribute",
    "document": "warhorse document",
    "lint": "warhorse lint",
    "pack": "warhorse pack",
    "process": "warhorse process",
    "publish": "warhorse publish",
    "test": "warhorse test",
    "jest": "jest",
    "jest-update": "jest --updateSnapshot"
  },
  "devDependencies": {
    "minami": "^1.2.3",
    "jest": "^20.0.4",
    "jest-html-reporter": "^0.1.0"
  },
  "jest-html-reporter": {
    "pageTitle": "Test Results",
    "outputPath": "docs/tests/index.html",
    "includeFailureMsg": false
  },
  "dependencies": {
    "tap": "^10.3.3",
    "tap-parser": "^5.3.3",
    "tape": "^4.6.3",
    "tinter": "^0.4.2"
  }
}
